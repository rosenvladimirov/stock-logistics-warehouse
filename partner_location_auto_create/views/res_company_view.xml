<?xml version="1.0"?>
<odoo>
    <data>
        <record id="view_company_partner_location_form" model="ir.ui.view">
            <field name="name">view.company.partner.location.form</field>
            <field name="model">res.company</field>
            <field name="inherit_id" ref="base.view_company_form"/>
            <field name="arch" type="xml">
                <xpath expr="//notebook[last()]" position="inside">
                    <page string="Comapny Logistics" name="logistics">
                        <group name="customer_supplier_location" string="Customer &amp; Supplier default location">
                            <field name="default_customer_location"/>
                            <field name="default_supplier_location"/>
                        </group>
                    </page>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
